{
  "expo": {
    "name": "footsteps",
    "slug": "footsteps",
    "privacy": "public",
    "entryPoint": "./target/index.js",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/footsteps_icon.png",
    "splash": {
      "image": "./assets/footsteps_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/sounds/*",
      "assets/*"
    ],
    "ios": {
      "bundleIdentifier": "argvader.footsteps",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "argvader.footsteps",
      "versionCode": 1
    }
  }
}
